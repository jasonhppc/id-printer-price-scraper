    "bodno.com": {
      "name": "Bodno",
      "base_url": "https://bodno.com",
      "search_url": "https://bodno.com/search?q={query}",
      "currency": "USD",
      "selectors": {
        "product_container": ".product-item, .grid__item, .product-wrap, .grid-product__content, .product",
