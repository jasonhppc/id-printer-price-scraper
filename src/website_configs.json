      "enabled": true
    },
    "amazon.com.au": {
      "name": "Amazon Australia",
      "base_url": "https://www.amazon.com.au",
      "search_url": "https://www.amazon.com.au/s?k={query}&ref=nb_sb_noss",
      "currency": "AUD",
      "selectors": {
        "product_container": "[data-component-type='s-search-result'], .s-result-item",
        "title": "h2 a span, .a-size-medium, .a-size-base-plus",
        "price": ".a-price-whole, .a-price .a-offscreen, .a-price-range",
        "link": "h2 a, .a-link-normal"
      },
      "enabled": true
    },
    "ebay.com.au": {
      "name": "eBay Australia",
      "base_url": "https://www.ebay.com.au",
      "search_url": "https://www.ebay.com.au/sch/i.html?_nkw={query}&_sacat=0",
      "currency": "AUD",
      "selectors": {
        "product_container": ".s-item, .srp-results .s-item",
        "title": ".s-item__title, h3",
        "price": ".s-item__price, .notranslate",
        "link": ".s-item__link"
      },
      "enabled": true
    },
    "idcardgroup.com": {
      "name": "ID Card Group (US)",
      "base_url": "https://www.idcardgroup.com",
      "search_url": "https://www.idcardgroup.com/catalogsearch/result/?q={query}",
      "currency": "USD",
      "selectors": {
        "product_container": ".product-item, .item, .product",
        "title": ".product-item-link, .product-name, h2, h3",
