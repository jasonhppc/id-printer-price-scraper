      },
      "enabled": true
    },
    "umart.com.au": {
      "name": "Umart Australia",
      "base_url": "https://www.umart.com.au",
      "search_url": "https://www.umart.com.au/search/?q={query}",
      "currency": "AUD",
      "selectors": {
        "product_container": ".product-item, .search-result-item",
